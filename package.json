{
  "name": "kit",
  "version": "2.0.9",
  "description": "A lightweight TypeScript utility library with Russian pluralization, object/array flattening, and utility types",
  "author": {
    "name": "teniryte",
    "email": "teniryte@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/teniryte/kit"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "devDependencies": {
    "typescript": "^5.9.2",
    "vite": "^7.0.6",
    "vitest": "^3.2.4"
  },
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./flatten": {
      "types": "./dist/esm/runtime/flatten.d.ts",
      "import": "./dist/esm/runtime/flatten.js",
      "require": "./dist/cjs/runtime/flatten.js"
    },
    "./primitive": {
      "types": "./dist/esm/types/primitive.d.ts",
      "import": "./dist/esm/types/primitive.js",
      "require": "./dist/cjs/types/primitive.js"
    },
    "./pluralize": {
      "types": "./dist/esm/runtime/pluralize.d.ts",
      "import": "./dist/esm/runtime/pluralize.js",
      "require": "./dist/cjs/runtime/pluralize.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "rm -rf dist && npm run build:esm && npm run build:cjs",
    "test": "vitest",
    "test:watch": "vitest watch",
    "coverage": "vitest run --coverage"
  },
  "type": "module",
  "types": "./dist/esm/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "dist/esm/index.d.ts"
      ],
      "flatten": [
        "dist/esm/runtime/flatten.d.ts"
      ],
      "primitive": [
        "dist/esm/types/primitive.d.ts"
      ],
      "pluralize": [
        "dist/esm/runtime/pluralize.d.ts"
      ]
    }
  }
}
