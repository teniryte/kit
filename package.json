{
  "name": "kit",
  "version": "2.0.18",
  "description": "A lightweight TypeScript utility library with Russian pluralization, object/array flattening, and utility types",
  "author": {
    "name": "teniryte",
    "email": "teniryte@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/teniryte/kit"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "devDependencies": {
    "@types/node": "^24.1.0",
    "typescript": "^5.9.2",
    "vite": "^7.0.6",
    "vitest": "^3.2.4"
  },
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./flatten": {
      "types": "./dist/esm/runtime/flatten.d.ts",
      "import": "./dist/esm/runtime/flatten.js",
      "require": "./dist/cjs/runtime/flatten.js"
    },
    "./primitive": {
      "types": "./dist/esm/types/primitive.type.d.ts",
      "import": "./dist/esm/types/primitive.type.js",
      "require": "./dist/cjs/types/primitive.type.js"
    },
    "./pluralize": {
      "types": "./dist/esm/runtime/pluralize.d.ts",
      "import": "./dist/esm/runtime/pluralize.js",
      "require": "./dist/cjs/runtime/pluralize.js"
    },
    "./kind": {
      "types": "./dist/esm/types/kind.enum.d.ts",
      "import": "./dist/esm/types/kind.enum.js",
      "require": "./dist/cjs/types/kind.enum.js"
    },
    "./kind-of": {
      "types": "./dist/esm/runtime/kind-of.d.ts",
      "import": "./dist/esm/runtime/kind-of.js",
      "require": "./dist/cjs/runtime/kind-of.js"
    },
    "./args-names": {
      "types": "./dist/esm/runtime/args-names.d.ts",
      "import": "./dist/esm/runtime/args-names.js",
      "require": "./dist/cjs/runtime/args-names.js"
    },
    "./event-emitter": {
      "types": "./dist/esm/runtime/event-emitter.d.ts",
      "import": "./dist/esm/runtime/event-emitter.js",
      "require": "./dist/cjs/runtime/event-emitter.js"
    },
    "./debug-string": {
      "types": "./dist/esm/runtime/debug-string.d.ts",
      "import": "./dist/esm/runtime/debug-string.js",
      "require": "./dist/cjs/runtime/debug-string.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "rm -rf dist && npm run build:esm && npm run build:cjs",
    "test": "vitest",
    "test:watch": "vitest watch",
    "coverage": "vitest run --coverage"
  },
  "type": "module",
  "types": "./dist/esm/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "dist/esm/index.d.ts"
      ],
      "flatten": [
        "dist/esm/runtime/flatten.d.ts"
      ],
      "primitive": [
        "dist/esm/types/primitive.type.d.ts"
      ],
      "pluralize": [
        "dist/esm/runtime/pluralize.d.ts"
      ],
      "kind": [
        "dist/esm/types/kind.enum.d.ts"
      ],
      "kind-of": [
        "dist/esm/runtime/kind-of.d.ts"
      ],
      "args-names": [
        "dist/esm/runtime/args-names.d.ts"
      ],
      "event-emitter": [
        "dist/esm/runtime/event-emitter.d.ts"
      ],
      "debug-string": [
        "dist/esm/runtime/debug-string.d.ts"
      ]
    }
  }
}
